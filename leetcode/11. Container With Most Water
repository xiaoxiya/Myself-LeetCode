class Solution {
    public int maxArea(int[] height) {
        int max = 0;
        int x,y=0;
        int len = height.length;
        for(int i=0; i<len;i++){
           for(int j=i+1;j<len;j++){
			y=height[i]<height[j] ? height[i]:height[j];
            x = j-i;
            max = max < x*y ? x*y: max;
           }
        }
        return max;
    }
}
